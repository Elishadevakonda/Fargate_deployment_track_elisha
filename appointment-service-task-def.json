{
  "family": "appointment-service-task",
  "executionRoleArn": "arn:aws:iam::539935451710:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::539935451710:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "appointment-service-Container",
      "image": "539935451710.dkr.ecr.us-east-1.amazonaws.com/appointment-service:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3001,
          "hostPort": 3001
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/appointment-service",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}

